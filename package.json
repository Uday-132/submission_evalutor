{
  "name": "submission-evaluator-mern",
  "version": "1.0.0",
  "description": "MERN stack submission evaluator for pitch decks and MSME proposals",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "install-client": "cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "officegen": "^0.6.5",
    "mammoth": "^1.6.0",
    "groq-sdk": "^0.3.3",
    "pdfkit": "^0.13.0",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "mern",
    "submission",
    "evaluator",
    "ai",
    "groq",
    "pitch-deck"
  ],
